{
	"appId": "com.vovagorodok.blichess",
	"appName": "blichess",
	"bundledWebRuntime": false,
	"webDir": "www",
	"backgroundColor": "000000ff",
	"appendUserAgent": "Lichobile/8.0.0+ble1.0.4",
	"plugins": {
		"SplashScreen": {
			"androidSplashResourceName": "launch_splash",
			"launchAutoHide": false,
			"useDialog": false
		},
		"PushNotifications": {
			"presentationOptions": [
				"sound",
				"alert"
			]
		}
	},
	"ios": {
		"scheme": "lichess"
	},
	"packageClassList": [
		"BluetoothLe",
		"KeepAwakePlugin",
		"AppPlugin",
		"CAPBrowserPlugin",
		"ClipboardPlugin",
		"DevicePlugin",
		"DialogPlugin",
		"FilesystemPlugin",
		"HapticsPlugin",
		"KeyboardPlugin",
		"CAPNetworkPlugin",
		"PreferencesPlugin",
		"SharePlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin",
		"ToastPlugin",
		"SoundEffectPlugin",
		"Stockfish",
		"StockfishVariants"
	]
}
