{
	"appId": "com.vovagorodok.blichess",
	"appName": "blichess",
	"bundledWebRuntime": false,
	"webDir": "www",
	"backgroundColor": "000000ff",
	"appendUserAgent": "Lichobile/8.0.0",
	"plugins": {
		"SplashScreen": {
			"androidSplashResourceName": "launch_splash",
			"launchAutoHide": false,
			"useDialog": false
		},
		"PushNotifications": {
			"presentationOptions": [
				"sound",
				"alert"
			]
		}
	},
	"ios": {
		"scheme": "lichess"
	}
}
